{
  "name": "boundless-sdk",
  "version": "0.1.0",
  "description": "SDK for OpenLayers 3 and React plus Flux",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/boundlessgeo/sdk.git"
  },
  "scripts": {
    "css": "node_modules/.bin/dr-frankenstyle dist/css",
    "minify:css": "node_modules/.bin/cleancss -o dist/css/components.min.css dist/css/components.css",
    "build:full": "node_modules/.bin/browserify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js]  js/full.jsx -o dist/js/full.js",
    "build:full:debug": "node_modules/.bin/browserify -g [envify --NODE_ENV development] js/full.jsx -o dist/js/full-debug.js",
    "build": "node_modules/.bin/npm-run-all --parallel css build:full:debug build:full",
    "docs": "node scripts/generate-api-doc.js",
    "test:mochify": "node_modules/.bin/mochify __tests__/**/*.test.js",
    "test:lint": "eslint js --ext .jsx,.js",
    "test": "node_modules/.bin/npm-run-all --parallel test:mochify test:lint"
  },
  "author": "Boundless Spatial, Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "blueimp-canvas-to-blob": "^2.2.0",
    "bootstrap": "3.3.5",
    "browser-filesaver": "^1.1.0",
    "c3-windows": "^0.4.11-rc4",
    "debounce": "^1.0.0",
    "filtrex": "^0.5.4",
    "fixed-data-table": "0.6.0",
    "flux": "^2.0.1",
    "font-awesome": "^4.4.0",
    "jspdf-browserify": "^1.0.272",
    "keymirror": "~0.1.0",
    "lodash.uniqueid": "^3.0.0",
    "ol3-cesium": "3.9.0",
    "openlayers": "3.11.1",
    "pui-react-alerts": "3.0.0-alpha.1",
    "pui-react-buttons": "3.0.0-alpha.1",
    "pui-react-dropdowns": "3.0.0-alpha.1",
    "pui-react-grids": "3.0.0-alpha.1",
    "pui-react-helpers": "3.0.0-alpha.1",
    "pui-react-iconography": "3.0.0-alpha.1",
    "pui-react-modals": "3.0.0-alpha.1",
    "pui-react-search-input": "3.0.0-alpha.1",
    "pui-react-tabs": "3.0.0-alpha.1",
    "pure-render-decorator": "^0.2.0",
    "react": "0.14.3",
    "react-color": "1.2.1",
    "react-dom": "0.14.3",
    "react-dropzone": "3.0.0",
    "react-intl": "2.0.0-beta-1",
    "react-slick": "0.9.1",
    "slick-carousel-dr-frankenstyle": "^1.5.8"
  },
  "devDependencies": {
    "babel-plugin-react-intl": "1.0.0-beta-5",
    "babelify": "^6.0.2",
    "browserify": "^11.2.0",
    "chai": "^1.10.0",
    "clean-css": "^3.4.6",
    "cssify": "^0.8.0",
    "dr-frankenstyle-win": "0.2.12",
    "envify": "3.4.0",
    "es6-promise": "^3.0.2",
    "eslint": "1.9.0",
    "eslint-plugin-react": "3.9.0",
    "intl": "^1.0.0",
    "minifyify": "^7.1.0",
    "mocha": "^2.1.0",
    "mochify": "^2.10.0",
    "npm-run-all": "^1.2.11",
    "phantomjs": "^1.9.16",
    "react-addons-test-utils": "0.14.3",
    "react-docgen": "^2.2.0",
    "sinon": "^1.12.2",
    "through": "^2.3.8",
    "watchify": "^3.2.0"
  },
  "browserify": {
    "transform": [
      "cssify",
      "babelify"
    ]
  }
}
