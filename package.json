{
  "name": "boundless-sdk",
  "version": "0.2.0",
  "description": "SDK for OpenLayers 3 and React plus Flux",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://repo.boundlessgeo.com/api/npm/npmjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/boundlessgeo/sdk.git"
  },
  "scripts": {
    "cover": "node_modules/.bin/mochify --plugin [ mochify-istanbul --report text --exclude **/js/constants/* --exclude **/js/**/*.css --exclude **/__tests__/**/* ] --transform ./scripts/test-transform.js __tests__/**/*.test.js",
    "postinstall": "node scripts/install.js",
    "i18n": "node scripts/generate-locale.js",
    "css:drfrankenstyle": "node_modules/.bin/dr-frankenstyle dist/css",
    "minify:css": "node_modules/.bin/cleancss -o dist/css/components.min.css dist/css/components.css",
    "css": "node_modules/.bin/npm-run-all css:drfrankenstyle minify:css",
    "build:full:debug": "node_modules/.bin/browserify -p licensify -g [envify --NODE_ENV development] js/full.js -o dist/js/full-debug.js",
    "build:full": "node_modules/.bin/browserify -p licensify -g [envify --NODE_ENV production] -p [minifyify --no-map --exclude node_modules/openlayers/dist/ol.js]  js/full.js -o dist/js/full.js",
    "build": "node_modules/.bin/npm-run-all --parallel css build:full build:full:debug",
    "generate:docs": "node scripts/generate-api-doc.js",
    "generate:gitbook": "node_modules/.bin/gitbook -v pre build",
    "generate:pdf": "node_modules/.bin/gitbook -v pre pdf",
    "docs": "node_modules/.bin/npm-run-all generate:docs generate:gitbook generate:pdf",
    "serve:gitbook": "node_modules/.bin/gitbook -v pre serve",
    "test:mochify:wd": "node_modules/.bin/mochify --transform ./scripts/test-transform.js --wd __tests__/**/*.test.js",
    "test:mochify": "node_modules/.bin/mochify --reporter list --transform ./scripts/test-transform.js __tests__/**/*.test.js",
    "test:lint": "eslint __tests__ js --ext .jsx,.js",
    "test": "node_modules/.bin/npm-run-all --parallel test:mochify test:lint"
  },
  "author": "Boundless Spatial, Inc.",
  "license": "Apache-2.0",
  "dependencies": {
    "blueimp-canvas-to-blob": "2.2.2",
    "browser-filesaver": "^1.1.0",
    "c3-windows": "^0.4.11-rc4",
    "debounce": "^1.0.0",
    "filtrex": "^0.5.4",
    "fixed-data-table": "0.6.0",
    "flux": "^2.0.1",
    "jsonix": "^2.4.1",
    "jspdf-browserify": "^1.0.272",
    "keymirror": "~0.1.0",
    "lodash.uniqueid": "^3.0.0",
    "material-ui": "^0.14.4",
    "ogc-schemas": "^2.6.0",
    "ol3-cesium": "3.9.4",
    "openlayers": "3.15.0",
    "pure-render-decorator": "^0.2.0",
    "react": "0.14.8",
    "react-color": "1.2.1",
    "react-dom": "0.14.8",
    "react-dropzone": "3.0.0",
    "react-intl": "2.0.0-rc-1",
    "react-responsive-decorator": "0.0.1",
    "react-slick": "0.9.1",
    "react-tap-event-plugin": "^0.2.2",
    "slick-carousel-dr-frankenstyle": "^1.5.8",
    "url-parse": "^1.1.1",
    "w3c-schemas": "^1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-plugin-react-intl": "1.0.0-beta-5",
    "babelify": "^6.0.2",
    "browserify": "^11.2.0",
    "chai": "^1.10.0",
    "clean-css": "^3.4.6",
    "cssify": "^0.8.0",
    "dr-frankenstyle-win": "0.2.12",
    "envify": "3.4.0",
    "es6-promise": "^3.0.2",
    "eslint": "1.9.0",
    "eslint-plugin-react": "3.9.0",
    "gitbook-cli": "2.1.2",
    "intl": "^1.0.0",
    "licensify": "^2.2.0",
    "minifyify": "^7.1.0",
    "mocha": "^2.1.0",
    "mochify": "^2.10.0",
    "mochify-istanbul": "^2.4.1",
    "npm-run-all": "^1.2.11",
    "phantomjs-prebuilt": "^2.1.7",
    "raf": "^3.2.0",
    "react-addons-test-utils": "0.14.8",
    "react-docgen": "^2.2.0",
    "sinon": "^1.12.2",
    "through": "^2.3.8",
    "watchify": "^3.2.0"
  },
  "browserify": {
    "transform": [
      "cssify",
      "babelify"
    ]
  }
}
