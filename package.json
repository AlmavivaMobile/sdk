{
  "name": "boundless-sdk",
  "version": "0.1.0",
  "description": "SDK for OpenLayers 3 and React plus Flux",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/boundlessgeo/sdk.git"
  },
  "scripts": {
    "start:tabbed": "node_modules/.bin/watchify js/tabbed.jsx -o dist/js/tabbed-debug.js",
    "start:basic": "node_modules/.bin/watchify js/basic.jsx -o dist/js/basic-debug.js",
    "start:server": "node_modules/.bin/st -nc",
    "css": "node_modules/.bin/dr-frankenstyle css",
    "start": "node_modules/.bin/npm-run-all --parallel css start:tabbed start:basic start:server",
    "build:tabbed": "node_modules/.bin/browserify -p [minifyify --no-map] js/tabbed.jsx -o dist/js/tabbed.js",
    "build:basic": "node_modules/.bin/browserify -p [minifyify --no-map]  js/basic.jsx -o dist/js/basic.js",
    "build": "node_modules/.bin/npm-run-all --parallel css build:tabbed build:basic",
    "docs": "node_modules/.bin/react-docgen js/components > api/info.json",
    "test:mochify": "node_modules/.bin/mochify js/**/*.test.js",
    "test:lint": "eslint js --ext .jsx,.js",
    "test": "node_modules/.bin/npm-run-all --parallel test:mochify test:lint"
  },
  "author": "Boundless",
  "license": "ISC",
  "dependencies": {
    "blueimp-canvas-to-blob": "^2.2.0",
    "bootstrap": "3.3.5",
    "browser-filesaver": "^1.1.0",
    "c3-windows": "^0.4.11-rc4",
    "filtrex": "^0.5.4",
    "fixed-data-table": "^0.4.6",
    "flux": "^2.0.1",
    "font-awesome": "^4.4.0",
    "jspdf-browserify": "^1.0.272",
    "keymirror": "~0.1.0",
    "lodash.uniqueid": "^3.0.0",
    "ol3-cesium": "^3.8.0",
    "openlayers": "~3.9.0",
    "pui-react-alerts": "^2.0.0-alpha.0",
    "pui-react-buttons": "^2.0.0-alpha.0",
    "pui-react-dropdowns": "^2.0.0-alpha.0",
    "pui-react-grids": "^2.0.0-alpha.0",
    "pui-react-helpers": "^2.0.0-alpha.0",
    "pui-react-iconography": "^2.0.0-alpha.0",
    "pui-react-modals": "^2.0.0-alpha.0",
    "pui-react-search-input": "^2.0.0-alpha.0",
    "pui-react-tabs": "^2.0.0-alpha.0",
    "react": "^0.13.2",
    "react-color-picker": "^2.1.9",
    "react-dropzone": "^2.0.1",
    "react-intl": "^2.0.0-pr-2",
    "react-slick": "^0.7.0",
    "slick-carousel-dr-frankenstyle": "^1.5.8"
  },
  "devDependencies": {
    "babel-plugin-react-intl": "^1.0.0-beta-4",
    "babelify": "^6.0.2",
    "browserify": "^11.2.0",
    "browserify-css": "^0.7.1",
    "chai": "^1.10.0",
    "dr-frankenstyle-win": "^0.2.11",
    "es6-promise": "^3.0.2",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^3.3.0",
    "intl": "^1.0.0",
    "minifyify": "^7.1.0",
    "mocha": "^2.1.0",
    "mochify": "^2.10.0",
    "npm-run-all": "^1.2.11",
    "phantomjs": "^1.9.16",
    "react-docgen": "^2.1.2",
    "sinon": "^1.12.2",
    "st": "^1.0.0",
    "watchify": "^3.2.0"
  },
  "browserify": {
    "transform": [
      "browserify-css",
      "babelify"
    ]
  }
}
